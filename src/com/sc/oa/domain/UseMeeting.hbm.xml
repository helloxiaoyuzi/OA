<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sc.oa.domain">

	<class name="UseMeeting" table="usemeeting">
		<id name="id">
			<generator class="native" />		
		</id>
		<property name="applyDate" type="java.util.Date" column="applyDate"/>
		<property name="startDate" type="java.util.Date" column="startDate"/>
		<property name="endDate" type="java.util.Date" column="endDate"/>
		<property name="state" type="integer" column="state"/>
		<property name="meetName"/>
		<property name="userName"/>
</class>
</hibernate-mapping>
